    <body class="home" id="up" data-spy="scroll" data-target=".fixed-top">

        <button class="d-block d-lg-none d-xl-none menu-toggle"></button>
        <nav class="mobile">
            <ul class="p-0 menu">
            <li data-text="Training">
                <a class="px-0 nav-link" href="#train">Train</a>
            </li>
            </ul>
